{
  "name": "vvc",
  "version": "10.0.0-alpha.1",
  "description": "Vivocha Command Line Tools",
  "keywords": [
    "vivocha",
    "cli",
    "command-line",
    "tools",
    "engagement"
  ],
  "homepage": "https://github.com/vivocha/vvc#readme",
  "bugs": {
    "url": "https://github.com/vivocha/vvc/issues"
  },
  "license": "MIT",
  "author": "Federico Pinna <fpinna@vivocha.com> (https://www.vivocha.com/)",
  "contributors": [
    "Federico Moretti <fmoretti@vivocha.com> (https://www.vivocha.com/)"
  ],
  "files": [
    "dist/**/*.js",
    "app/bg.svg",
    "app/index.html",
    "app/vivocha_widget_tester.js",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "vvc": "./dist/vvc.js",
    "vvc-info": "./dist/vvc-info.js",
    "vvc-login": "./dist/vvc-login.js",
    "vvc-logout": "./dist/vvc-logout.js",
    "vvc-strings": "./dist/vvc-strings.js",
    "vvc-widget": "./dist/vvc-widget.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivocha/vvc.git"
  },
  "scripts": {
    "clean": "rm -fr dist/ coverage .nyc_output node_modules/ app/vivocha_widget_tester.js",
    "prebuild": "rm -fr dist/",
    "build": "tsc && tsc -p app && webpack --config app/webpack.config.js --context app",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "watch:test": "npm t -- -w",
    "test": "mocha test/*.test.js",
    "cover": "nyc --reporter=lcov --reporter=text npm t",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "@vivocha/public-entities": "^8.4.7",
    "arrest": "^13.2.5",
    "body-parser": "^2.2.0",
    "columnify": "^1.6.0",
    "commander": "^14.0.0",
    "eredita": "^1.2.1",
    "express": "^5.1.0",
    "jsonpolice": "^12.0.0",
    "lodash": "^4.17.21",
    "openurl": "^1.1.1",
    "pofile": "^1.1.4",
    "reload": "^3.3.0",
    "semver": "^7.7.2",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.17",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.31",
    "@types/reload": "^3.2.3",
    "@types/request": "^2.48.12",
    "@types/semver": "^7.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "commitizen": "^3.0.4",
    "coveralls": "^3.0.2",
    "husky": "^1.1.2",
    "mocha": "^11.6.0",
    "nyc": "^17.1.0",
    "sass": "^1.89.2",
    "semantic-release": "^24.2.5",
    "typescript": "^5.8.3"
  }
}
